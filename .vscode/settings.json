{
  "makefile.extensionOutputFolder": "./.vscode",
  "C_Cpp.default.compilerPath": "clang",
  "editor.defaultFormatter": "xaver.clang-format",
  "files.associations": {
    "*.asm": "asm-intel-x86-generic",
    "string.h": "c",
    "functional": "c",
    "string": "c",
    "optional": "c",
    "ratio": "c",
    "system_error": "c",
    "array": "c",
    "tuple": "c",
    "type_traits": "c",
    "utility": "c",
    "variant": "c",
    "__bit_reference": "c",
    "__node_handle": "c",
    "bitset": "c",
    "chrono": "c",
    "deque": "c",
    "__memory": "c",
    "limits": "c",
    "random": "c",
    "vector": "c",
    "stddef.h": "c"
  },
  "python.defaultInterpreterPath": "python",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}